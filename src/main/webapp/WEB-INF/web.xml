<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <listener>
        <listener-class>com.darksoulsdeaths.StartClass</listener-class>
    </listener>

    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/index.html</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>stats</servlet-name>
        <jsp-file>/stats.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>results</servlet-name>
        <jsp-file>/results.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>about</servlet-name>
        <jsp-file>/about.html</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>FAQ</servlet-name>
        <jsp-file>/FAQ.html</jsp-file>
    </servlet>

    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>stats</servlet-name>
        <url-pattern>/stats</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>results</servlet-name>
        <url-pattern>/results</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>about</servlet-name>
        <url-pattern>/about</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FAQ</servlet-name>
        <url-pattern>/FAQ</url-pattern>
    </servlet-mapping>
</web-app>
